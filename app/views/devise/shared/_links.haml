- if controller_name != 'sessions'
  = link_to "Log ind", new_session_path(resource_name)
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations' && @places_left > ZERO
  = link_to "Tilmeld", new_registration_path(resource_name)
  %br/
- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations'
  = link_to "Glemt password?", new_password_path(resource_name)
  %br/
- if devise_mapping.confirmable? && controller_name != 'confirmations'
  = link_to "Ikke modtaget bekræftelsesinstruktion?", new_confirmation_path(resource_name)
  %br/
- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
  = link_to "Ikke modtaget info for at låse din konto op?", new_unlock_path(resource_name)
  %br/
- if devise_mapping.omniauthable?
  - resource_class.omniauth_providers.each do |provider|
    = link_to "Log ind med #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider)
    %br/
