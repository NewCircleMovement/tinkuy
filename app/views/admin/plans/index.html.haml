.row
  .col-md-12
    %h1 Medlemsskaber
  - @plans.each do |plan|
    .col-md-3
      .well
        %h4= plan.name
        %p
          %strong= plan.price
          Kroner
        %p
          = link_to 'Edit', edit_admin_plan_path(plan)
          = link_to 'Destroy', admin_plan_path(plan), :method => :delete, :data => { :confirm => 'Are you sure?' }

  .col-md-12
    = link_to 'Ny plan', new_admin_plan_path
